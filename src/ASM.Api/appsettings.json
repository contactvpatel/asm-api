{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "ConsoleLog": {
      "Enabled": false
    },
    "FileLog": {
      "Enabled": false
    },
    "ElasticLog": {
      "Enabled": false,
      "Url": "http://localhost:9200"
    }
  },
  "AllowedHosts": "*",
