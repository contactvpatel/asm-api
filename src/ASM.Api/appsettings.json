  },
  "MisService": {
    "Url": "https://qa-misapi.baps.dev/public_IT_Review/api/V2/",
    "Endpoint": {
      "Department": "Department",
      "Role": "Role",
      "Position": "Position",
      "PersonPosition": "PersonPosition"
    }
  },
  "SsoService": {
    "Url": "https://qa-misapi.baps.dev/public_IT_Review/api/V2/",
    "Endpoint": {
      "Application": "Department"
    }
